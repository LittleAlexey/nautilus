{
  "name": "nautilus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "material-ui": "^1.0.0-beta.24",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-pagination": "^1.1.6",
    "moment": "^2.20.1",
    "rc-pagination": "^1.15.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-image-gallery": "^0.8.6",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "exit 0",
    "eject": "react-scripts eject"
  },
  "standard": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "rules": {
      "comma-dangle": 0,
      "react/jsx-uses-vars": 1,
      "react/display-name": 1,
      "no-unused-vars": "warn",
      "no-console": 1,
      "no-unexpected-multiline": "warn"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "15.6.1"
      }
    },
    "ignore": [
      "src/registerServiceWorker.js"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.1"
  }
}
